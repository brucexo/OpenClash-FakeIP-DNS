ruleset=🎯 广告拦截,https://raw.githubusercontent.com/blackmatrix7/ios_rule_script/master/rule/Clash/Advertising/Advertising.list


# 2. 基础节点组 (定义自动测速的“零件”)
#---------------------------------------------------
# custom_proxy_group=名称`类型`正则表达式`测速地址`测速间隔`容差
custom_proxy_group=🛡️ US-自建`url-test`SelfHosted`http://www.gstatic.com/generate_204`300
# 注：此处筛选逻辑为 "包含 US|美国" 且 "不包含 SelfHosted"，具体写法可能因转换工具而异。
# 以下为通用写法，通常需要在工具UI中额外设置排除关键词 "SelfHosted"。
custom_proxy_group=🇺🇸 US-机场`url-test`(?i)US|美国`http://www.gstatic.com/generate_204`300
custom_proxy_group=🇭🇰 香港-自动`url-test`(?i)HK|港`http://www.gstatic.com/generate_204`300
custom_proxy_group=🇹🇼 台湾-自动`url-test`(?i)TW|台`http://www.gstatic.com/generate_204`300
custom_proxy_group=🇯🇵 日本-自动`url-test`(?i)JP|日`http://www.gstatic.com/generate_204`300
custom_proxy_group=🇸🇬 新加坡-自动`url-test`(?i)SG|新`http://www.gstatic.com/generate_204`300
custom_proxy_group=🌏 亚洲-自动`url-test`(?i)HK|港|TW|台|JP|日|SG|新`http://www.gstatic.com/generate_204`300
# 注：此处应排除自建节点，通常需要在工具UI中额外设置排除关键词 "SelfHosted"。
custom_proxy_group=🌍 全球-自动`url-test`.*`http://www.gstatic.com/generate_204`300


# 3. 功能策略组 (定义手动调试的“菜单”)
#---------------------------------------------------
# custom_proxy_group=名称`类型`[]策略1[]策略2[]...
custom_proxy_group=🧠 AI & Google`select`[]🛡️ US-自建`[]🇺🇸 US-机场`[]DIRECT`[]🇯🇵 日本-自动`[]🌍 全球-自动
custom_proxy_group=🎬 流媒体`select`[]🌏 亚洲-自动`[]🛡️ US-自建`[]🇺🇸 US-机场`[]🇭🇰 香港-自动`[]🇹🇼 台湾-自动`[]🇯🇵 日本-自动`[]🇸🇬 新加坡-自动`[]DIRECT
custom_proxy_group=💬 社交&通讯`select`[]🌍 全球-自动`[]🌏 亚洲-自动`[]🛡️ US-自建`[]🇺🇸 US-机场`[]DIRECT
custom_proxy_group=🌐 其他国外`select`[]🌍 全球-自动`[]🛡️ US-自建`[]🇺🇸 US-机场`[]DIRECT
# 注：转换工具通常会自动创建与ruleset同名的select组，如果工具支持，上面几行可省略。
# 为了兼容性，这里明确写出。

# 核心功能组
custom_proxy_group=🎯 广告拦截`select`[]REJECT`[]DIRECT
custom_proxy_group=🇨🇳 国内网站`select`[]DIRECT`[]🧠 AI & Google
custom_proxy_group=🚀 最终选择`select`[]🌐 其他国外`[]🧠 AI & Google`[]🎬 流媒体`[]💬 社交&通讯`[]DIRECT`[]🛡️ US-自建`[]🇺🇸 US-机场`[]🇭🇰 香港-自动`[]🇹🇼 台湾-自动`[]🇯🇵 日本-自动`[]🇸🇬 新加坡-自动


# 4. 启用规则生成器 (总开关)
#---------------------------------------------------
enable_rule_generator=true
overwrite_original_rules=true
